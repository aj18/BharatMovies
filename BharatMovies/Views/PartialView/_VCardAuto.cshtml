@using System.Configuration;
@{
  
    var returnUrl = ConfigurationManager.AppSettings.Get("returnUrl");
    var returnUrl2 = ConfigurationManager.AppSettings.Get("returnUrl2");
    var returnUrl3 = ConfigurationManager.AppSettings.Get("returnUrl3");
}
<div id="vcardm"></div>
<script type="text/javascript">
    var Bubbly = new BubblyCard();

    var data =@(Html.Raw(ViewBag.Summary));
    var ID ="@(Html.Raw(ViewBag.ID))";
    var returnUrl="@(Html.Raw(returnUrl))"
    var returnUrl2="@(Html.Raw(returnUrl2))"
    var returnUrl3="@(Html.Raw(returnUrl3))"
    var paraVCardM = {
        "campaignId": ID,
        "cardType": "VCARDAUTO",
        "returnUrl": returnUrl + "?id=" + ID,
        "styles": {
            panelStyle: { 'padding': '0px;', 'height': '502px;', 'margin': '0px;' },
            titleStyle: {
                'background-color': 'rgba(145, 205, 40, 0.85)!important;',
                'font-size': '16px;', 'color': '#fff;', 'font-weight': 'normal !important;'
            },
            subTitleStyle: {
                'background-color': 'rgba(255, 255, 255, 0.65);',
                'font-size': '14px;', 'color': '#000;', 'bottom': '65px !important;', 'position': 'relative;', 'font-weight': 'normal !important;'
            },
            pictureStyle: { 'width': '100%;', 'height': '502px !important;', 'border': 'none !important;', },
            iconStyle: { 'color': 'rgba(75, 188, 150, 0.85);' }
        },
        "clientId": "100"
    };

    Bubbly.showCard(paraVCardM, data, document.getElementById('vcardm'));

</script>